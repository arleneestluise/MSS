SELECT [DIM_MarketingTacticId]
      ,[Campaign]
      ,[TacticName]
      ,[MarketingTacticId]
      ,[MarketingPlanningActivityId]
      ,[EpcId]
      ,[TacticArea]
      ,[MarketingTacticType]
      ,[MarketingTacticCountry]
      ,[FunnelName]
      ,[FriendlyTacticName]
      ,[GlobalVsLocal]
      ,[MarketingPlanningGlobalVsLocal]
      ,[Product]
      ,[MarketingPlay]
      ,[SolutionPlay]
      ,[MarketingPlayType]
      ,[MarketingPlaySummaryProduct]
      ,[MarketingPlaySolutionArea]
      ,[MarketingScenario]
      ,[MarketingTacticCategory]
      ,[GDCProgram]
      ,[NonMarketingPlayCampaign]
      ,[TacticCreatedDate]
      ,[TacticStartDate]
      ,[TacticEndDate]
      ,[Attribution_Flag]
      ,[DeletedTactic_Flag]
      ,[BuyerJourneyStage]
      ,[EventSeries]
      ,[PrimaryLanguage]
      ,[BuiltOnDate]
      ,[TargetTacticName]
      ,[TargetTacticId]
      ,[TacticCountryAddress]
      ,[TacticType]
      ,[MarketingPlayProduct]
      ,[ActiveMarketingPlay_Flag]
      ,[ScenarioType]
      ,[SolutionPlayAligned_Flag]
      ,[WorkspaceName]
      ,[LandingPageUrl]
      ,[TacticDownload]
      ,[EmailPrimaryCTA]
      ,[TrialPaymentInstrument]
      ,[TrialSetupType]
      ,[ContentInitiative]
      ,[MarketingProgram]
      ,[MarketingSubProgram]
  FROM [Conformed_Mktg].[dim_marketingtactic]
  WHERE [TacticCreatedDate] >= '2024-12-01'
